POSTGRES_USER=dhadgar
POSTGRES_PASSWORD=dhadgar
POSTGRES_DB=dhadgar_platform

RABBITMQ_DEFAULT_USER=dhadgar
RABBITMQ_DEFAULT_PASS=dhadgar

REDIS_PASSWORD=dhadgar

# Observability
NEW_RELIC_LICENSE_KEY=
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Identity -> Key Vault (for stable signing keys)
AUTH_USE_DEV_CERTS=true
AUTH_KEYVAULT_URI=
AUTH_KEYVAULT_SIGNING_CERT_NAME=
AUTH_KEYVAULT_ENCRYPTION_CERT_NAME=
AZURE_TENANT_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=

# OpenIddict WIF audience (Entra token exchange)
OPENIDDICT_WIF_AUDIENCE=api://AzureADTokenExchange

# OAuth Providers (from respective developer portals)
OAUTH_DISCORD_CLIENT_ID=
OAUTH_DISCORD_CLIENT_SECRET=

# Microsoft OAuth (uses federated credentials - no client secret needed)
# App registration must have federated credential trusting Identity provider
OAUTH_MICROSOFT_CLIENT_ID=

# BetterAuth exchange private key (ES256/P-256 PEM format)
# Generate with: openssl ecparam -name prime256v1 -genkey -noout | openssl pkcs8 -topk8 -nocrypt
# Store as single line with literal \n for newlines
BETTERAUTH_EXCHANGE_PRIVATE_KEY=
